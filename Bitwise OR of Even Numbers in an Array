int evenNumberBitwiseORs(int* nums, int numsSize) {
    int j=0,even[100];
    for(int i=0;i<numsSize;i++)
    {
        if(nums[i]%2==0)
        {
            even[j]=nums[i];
            j++;
        }
    }
    int res=0;
    for(int i=0;i<j;i++)
    {
        res=res|even[i];
    }
    return res;
}
